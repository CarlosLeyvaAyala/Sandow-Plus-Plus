    "highlight.regexes": {
        "(; ?TODO:?)(.*)": [
            {
                "overviewRulerColor": "#ffcc00",
                "backgroundColor": "#ffcc00",
                "color": "#1f1f1f",
                "fontWeight": "bold",
                "overviewRulerLane": "4"
            },
            {
                "backgroundColor": "#d9ad00",
                "color": "#1f1f1f"
            }
        ],
        "(; ?Region:?)(.*)": [
            {
                "backgroundColor": "#117700",
                "color": "#1b1b1b",
                "fontWeight": "bold",
            },
            {
                "backgroundColor": "#228800",
                "color": "#1b1b1b",
            }
        ],
        "(;>)(.*)": [
            {
                "overviewRulerLane": "7",
                "overviewRulerColor": "#61500C",
                "backgroundColor": "#7d6608",
                "color": "#1f1f1f",
                "fontWeight": "bold"
            },
            {
                "fontWeight": "bold",
                "backgroundColor": "#7d6608",
                "color": "#1f1f1f"
            }
        ],
        "(;@.*:)(.*)": [
            {
                "overviewRulerColor": "tomato",
                "backgroundColor": "tomato",
                "color": "#1f1f1f",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "tomato",
                "color": "#1f1f1f"
            }
        ],
        "(; ?WARNING:?)(.*)": [
            {
                "backgroundColor": "goldenrod",
                // "textDecoration": "underline dotted orange",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "goldenrod",
                // "fontWeight": "bold"
            }
        ],
        "(~~ *)(.*)": [
            {
            },
            {
                // "backgroundColor": "goldenrod",
                "textDecoration": "line-through",
            }
        ],
        "(; ?FIXME:?)(.*)": [
            {
                "overviewRulerColor": "#ff0000",
                "backgroundColor": "#aa0000",
                "color": "#efefef",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "#aa0000",
                "color": "#efefef"
            }
        ],
